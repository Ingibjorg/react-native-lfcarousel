{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "installedESLint": true,
    "env": {
        "jest": true,
        "es6": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
      "react/jsx-filename-extension": "off"
    },
    "globals": {
      "document": true
    }
}
